{
  "expo": {
    "name": "ProgressiveChicago",
    "description": "Mobile App inteded for the members of Progressive Baptist Church.",
    "slug": "ProgressiveChicago",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.5.0",
    "orientation": "portrait",
    "icon": "./assets/pbc_icon.png",
    "splash": {
      "image": "./assets/pbc_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2e2e2e"
    },
    "ios": {
      "bundleIdentifier": "com.progressivechicago.progressive",
      "supportsTablet": false,
      "appStoreUrl" : "https://itunes.apple.com/us/app/progressivechicago/id1336476161"
    },
    "android": {
      "package": "com.progressivechicago.progressive",
      "playStoreUrl" : "https://play.google.com/store/apps/details?id=com.progressivechicago.progressive"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 30000
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    }
  }
}
